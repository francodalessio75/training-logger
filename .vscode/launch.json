{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Nest.js App",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run",
          "start:debug"
        ],
        "port": 9229,
        "skipFiles": ["<node_internals>/**"],
        "outFiles": ["${workspaceFolder}/dist/**/*.js"],
        "sourceMaps": true,
        "restart": true,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  }
  